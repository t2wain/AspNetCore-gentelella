<!DOCTYPE html>
<html lang="en">
<head>
    @if (IsSectionDefined("head"))
    {
        @RenderSection("head", required: false)
    }
    else
    {
        @if (IsSectionDefined("meta"))
        {
            @RenderSection("meta", required: false)
        }
        else
        {
            <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
            <!-- Meta, title, CSS, favicons, etc. -->
            <link rel="icon" href="~/images/favicon.ico" type="image/ico" />
            <meta charset="utf-8">
            <meta http-equiv="X-UA-Compatible" content="IE=edge">
            <meta name="viewport" content="width=device-width, initial-scale=1">
        }

        <title>Gentelella Alela! | @ViewData["Title"]</title>

        @if (IsSectionDefined("stylesheets"))
        {
            @RenderSection("stylesheets", required: false);
        }
        else
        {
            <!-- Bootstrap -->
            <link href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
            <!-- Font Awesome -->
            <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
            <!-- NProgress -->
            <link href="https://cdnjs.cloudflare.com/ajax/libs/nprogress/0.2.0/nprogress.min.css" rel="stylesheet">
            <!-- bootstrap-daterangepicker -->
            <link href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-daterangepicker/2.1.24/daterangepicker.min.css" rel="stylesheet">
            <!-- iCheck -->
            <link href="https://cdnjs.cloudflare.com/ajax/libs/iCheck/1.0.2/skins/flat/green.css" rel="stylesheet">
            <!-- bootstrap-progressbar -->
            <link href="https://unpkg.com/bootstrap-progressbar@0.9.0/css/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet">
            <!-- JQVMap -->
            <link href="https://cdnjs.cloudflare.com/ajax/libs/jqvmap/1.5.1/jqvmap.min.css" rel="stylesheet" />

            @RenderSection("stylesheets_ext", required: false)
        }
        <!-- Custom Theme Style -->
        <link href="~/static/css/custom.css" rel="stylesheet">
        @RenderSection("head_ext", required: false)
    }
</head>

<body class="nav-md @ViewData["sidebar_class"]">
    @if (IsSectionDefined("body"))
    {
        @RenderSection("body")
    }
    else
    {
        <div class="container body">
            <div class="main_container">

                @if (IsSectionDefined("sidebar"))
                {
                    @RenderSection("sidebar", required: false)
                }
                else
                {
                <div class="col-md-3 left_col @RenderSection("sidebar_class", required: false)">
                    <partial name="~/Views/Shared/site_template/sidebar.cshtml" />
                </div>
                    @RenderSection("sidebar_ext", required: false)
                }

                @if (IsSectionDefined("top_navigation"))
                {
                    @RenderSection("top_navigation", required: false)
                }
                else
                {
                <div class="top_nav">
                    <partial name="~/Views/Shared/site_template/top_navigation.cshtml" />
                </div>
                    @RenderSection("top_navigation_ext", required: false)
                }

                @RenderBody()

                @if (IsSectionDefined("footer"))
                {
                    @RenderSection("footer", required: false)
                }
                else
                {
                    <footer>
                        <partial name="~/Views/Shared/site_template/footer.cshtml" />
                    </footer>
                    @RenderSection("footer_ext", required: false)
                }
            </div>
        </div>

        @if (IsSectionDefined("javascripts"))
        {
            @RenderSection("javascripts", required: false)
        }
        else
        {
            <!-- jQuery -->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>
            <!-- Bootstrap -->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/js/bootstrap.min.js"></script>
            <!-- FastClick -->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/fastclick/1.0.6/fastclick.min.js"></script>
            <!-- NProgress -->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/nprogress/0.2.0/nprogress.min.js"></script>
            <!-- bootstrap-progressbar -->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-progressbar/0.9.0/bootstrap-progressbar.min.js"></script>
            <!-- iCheck -->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/iCheck/1.0.2/icheck.min.js"></script>
            <!-- bootstrap-daterangepicker -->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.13.0/moment.min.js"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-daterangepicker/2.1.24/daterangepicker.min.js"></script>
            <!-- bootstrap-wysiwyg -->
            <script src="https://unpkg.com/bootstrap-wysiwyg@1.0.4/js/bootstrap-wysiwyg.min.js"></script>
            <script src="https://unpkg.com/jquery.hotkeys@0.1.0/jquery.hotkeys.js"></script>
            <script src="https://unpkg.com/google-code-prettify@1.0.1/src/prettify.js"></script>
            <!-- jQuery Tags Input -->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-tagsinput/1.3.6/jquery.tagsinput.min.js"></script>
            <!-- Switchery -->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/switchery/0.8.1/switchery.min.js"></script>
            <!-- Select2 -->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/js/select2.full.min.js"></script>
            <!-- Autosize -->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/autosize.js/3.0.15/autosize.min.js"></script>
            <!-- jQuery autocomplete -->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.devbridge-autocomplete/1.2.24/jquery.autocomplete.min.js"></script>
            <!-- starrr -->
            <script src="https://unpkg.com/starrr@2.0.1/dist/starrr.js"></script>
            @RenderSection("javascripts_ext", required: false)
        }
        <!-- Custom Theme Scripts -->
        <script src="~/static/js/custom.js"></script>
    }
</body>
</html>
